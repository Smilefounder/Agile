<script src="@Url.Content("~/Scripts/jquery-1.8.2.js")"></script>
<script type="text/javascript">
    $(function () {
        $(".panel-heading").click(function () {
            var hide = $(this).attr("data-hide");
            if (hide) {
                $(this).siblings().show();
                $(this).removeAttr("data-hide");
                return;
            }

            $(this).siblings().hide();
            $(this).attr("data-hide", "1");
        });
    });
</script>