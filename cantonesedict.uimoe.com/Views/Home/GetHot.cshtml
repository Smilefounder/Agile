@model IEnumerable<Agile.Dtos.GroupItemDto>
@{
    Layout = "~/Views/Shared/_WithBottomAppBarLayout.cshtml";
}

<div class="weui_tab_bd">
    <div class="weui_cells weui_cells_access">
        @if (Model == null || Model.Count() == 0)
        {
            <a href="javascript:void(0)" class="weui_cell">
                <div class="weui_cell_bd weui_cell_primary">
                    <p>没有找到相关记录哦</p>
                </div>
                <div class="weui_cell_ft">
                </div>
            </a>
        }
        else
        {
            foreach (var item in Model)
            {
                <a href="javascript:void(0)" class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>
                            <div>@item.IName</div>
                        </p>
                    </div>
                    <div class="weui_cell_ft"><span>@Convert.ToInt32(item.ICount)</span>次查询</div>
                </a>
            }
        }
    </div>
</div>



<script type="text/javascript">
    window.onload = function () {
        showhighlight("appbaritem2");
    }
</script>
