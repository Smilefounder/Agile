@model Agile.API.Dtos.H10007Response
@{
    Layout = null;
}

<div class="list-group">
    <a class="list-group-item active">文章归档</a>
    @if (Model.data == null || Model.data.Count == 0)
    {
        <a class="list-group-item">未找到相关记录</a>
    }
    else
    {
        foreach (var item in Model.data)
        {
            <a class="list-group-item" href="javascript:void(0)" onclick="linkto('@Url.Content("~/")'+'Home/Index?page='+@ViewData["page"]+'&pagesize='+@ViewData["pagesize"]+'&yearmonth='+'@item.name')">
                <span>@item.name</span>(<span>@item.cnt</span>)
            </a>
        }
    }
</div>