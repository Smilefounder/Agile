<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width,target-densitydpi=high-dpi,initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    @RenderSection("styles", false)
    <style type="text/css">
        .newline {
            width: 100%;
            height: 10px;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-default">
        <div style="width:1025px;margin:0 auto;">
            <div style="float:left;">
                <a class="navbar-brand" href="@Url.Content("~/Home/Index")">吃吃吃！</a>
            </div>
            <div style="float:right;">
                <a class="navbar-brand" target="_blank" href="@Url.Content("~/Home/PostImg")">投稿</a>
                <a class="navbar-brand" target="_blank" href="http://weibo.com/ssitmo">微博</a>
            </div>
            <div style="clear:both;"></div>
        </div>
    </nav>
    <div style="width:1025px;margin:0 auto;">
        @RenderBody()
    </div>
</body>
</html>
<script src="@Url.Content("~/Scripts/jquery-2.2.3.min.js")"></script>
@RenderSection("scripts", false)
